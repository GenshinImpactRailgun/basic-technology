<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.frame.springboot.shirodemo.dao.UserTestMapper">
    <select id="getUserByUsername" resultType="com.frame.springboot.shirodemo.pojo.UserTest"
            parameterType="java.lang.String">
        select *
        from user_test
        where username = #{username};
    </select>
</mapper>
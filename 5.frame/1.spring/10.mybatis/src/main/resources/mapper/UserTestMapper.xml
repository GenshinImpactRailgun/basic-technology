<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.frame.spring.mybatis.dao.UserTestMapper">
    <select id="selectUserTestById" resultType="com.frame.spring.mybatis.pojo.UserTest">
        select *
        from user_test
        where id = #{id}
    </select>
    <select id="selectUserTestAll" resultType="com.frame.spring.mybatis.pojo.UserTest">
        select *
        from user_test
    </select>
</mapper>